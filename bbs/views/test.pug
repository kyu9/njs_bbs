doctype html
html
    head
        meta(charset='utf-8')
        title My Board
        style.
            table {
                border: 1px solid black;
                border-collapse: collapse;
            }

            table th, table td {
                border: 1px solid black;
            }
    body
        table#user-list
            thead
                tr
                    th ID
                    th 이름
                    th password
                    th created_time
            tbody
                for user in users
                    tr
                        td= user.id
                        td= user.name
                        td= user.password
                        td= user.created_at
        br